[include]
  path = ~/.gituser
[alias]
  br = branch
  ci = commit
  co = checkout
  df = diff
  rb = rebase
  st = status
  sa = stash
  mt = mergetool
  cp = cherry-pick
  sm = submodule
  pl = pull --rebase
  ps = push
  pf = push --force-with-lease
  branches = for-each-ref --sort=-committerdate --format=\"%(color:blue)%(authordate:relative)\t%(color:red)%(authorname)\t%(color:white)%(color:bold)%(refname:short)\" refs/remotes
  nuke = !sh -c 'git branch -D $1 && git push --no-verify origin :$1' -
  gone = !git pl && git fetch -p && git branch -vv | grep 'origin/.*: gone]' | awk '{print $1}' | xargs git branch -d
[core]
	excludesfile = /Users/schmiddi/.gitignore_global
  mergeoptions = "--no-edit"
	precomposeunicode = false
  editor = "/usr/local/bin/vim"
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path =
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[push]
	default = current
[color]
	ui = auto
	branch = auto
	diff = auto
	interactive = auto
	status = auto
	ui = true
	ui = true
[rebase]
  autosquash = true
[fetch]
  prune = true
[merge]
  ff = only
[url "git@codevault.io:"]
  insteadOf = https://codevault.io/
